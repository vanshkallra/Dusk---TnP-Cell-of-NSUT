{
    "version": 2,                     
    "builds": [
      {
        "src": "index.html",          
        "use": "@vercel/static"       
      },
      {
        "src": "Drives/index.php",    
        "use": "@vercel/php" 
      }          
    ],
    "routes": [
      {
        "src": "/Drives/(.*)",         
        "dest": "/Drives/index.php"    
      },
      {
        "src": "/SProfile/(.*)",         
        "dest": "/SProfile/index.php"     
      },
      {
        "src": "/HODProfile/(.*)",       
        "dest": "/HODProfile/index.php"   
      },
      {
        "src": "/PProfile/(.*)",         
        "dest": "/PProfile/index.php"     
      }
    ],
    "env": {
      "MY_ENV_VARIABLE": "value"      
    }
  }
  